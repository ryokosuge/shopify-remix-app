// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/dev.db"
}

model Shop {
  id              String @id @default(uuid())
  myshopifyDomain String @unique
  accessToken     String
}
